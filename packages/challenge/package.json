{
    "name": "@easy-community-spam-blocker/challenge",
    "version": "0.0.0",
    "description": "EasyCommunitySpamBlocker challenge implementation for Plebbit",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "package.json"
    ],
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "@noble/ed25519": "1.7.5",
        "@plebbit/plebbit-js": "https://github.com/plebbit/plebbit-js#3a70ec2b97a4fe2ed53f7746d69e9ec6a00dc324",
        "@easy-community-spam-blocker/shared": "file:../shared",
        "cborg": "4.3.2",
        "uint8arrays": "5.1.0",
        "zod": "4.3.5"
    },
    "devDependencies": {
        "typescript": "5.9.3",
        "vitest": "4.0.16"
    }
}
